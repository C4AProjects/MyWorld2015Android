        <item>De meilleures offres d\'emploi</item>
        <!-- 1 -->
        <item>Une bonne éducation</item>
        <!-- 2 -->
        <item>Un meilleur système de santé</item>
        <!-- 3 -->
        <item>Une alimentation nourrissante à un prix abordable</item>
        <!-- 4 -->
        <item>Un soutien à ceux qui ne peuvent pas travailler</item>
        <!-- 5 -->
        <item>Un accès au téléphone et à Internet</item>
        <!-- 6 -->
        <item>Un accès à l\'eau potable et à l\'assainissement</item>
        <!-- 7 -->
        <item>Un approvisionnement en énergie fiable chez soi</item>
        <!-- 8 -->
        <item>La protection des forêts, des rivières et des océans</item>
        <!-- 9 -->
        <item>La prise de mesures contre le changement climatique</item>
        <!-- 10 -->
        <item>Un gouvernement réactif et honnête</item>
        <!-- 11 -->
        <item>Les libertés politiques</item>
        <!-- 12 -->
        <item>La protection contre le crime et la violence</item>
        <!-- 13 -->
        <item>L\'égalité entre les hommes et les femmes</item>
        <!-- 14 -->
